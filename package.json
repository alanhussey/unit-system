{
  "name": "unit-system",
  "version": "2.0.0",
  "description": "safely and easily calculate with dimensioned data",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/umd/index.js",
  "types": "lib/index.d.ts",
  "license": "ISC",
  "keywords": [
    "unit",
    "units",
    "measure",
    "measurement",
    "convert",
    "conversion",
    "calculate",
    "calculations",
    "dimensions",
    "dimensional analysis"
  ],
  "scripts": {
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint ./src/**/*.ts",
    "clean": "rimraf ./lib",
    "build": "npm run clean && rollup -c rollup.config.ts",
    "pretest": "npm run lint",
    "test": "jest test",
    "prepublishOnly": "npm run build && npm test"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-check": "^2.14.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
